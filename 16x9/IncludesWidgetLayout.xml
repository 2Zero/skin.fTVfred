			<control type="image">
				<width>318</width>
				<height>334</height>
				<bordersize>0,13,10,13</bordersize>
				<aspectratio align="center">keep</aspectratio>
				<texture background="true" fallback="views/pvr_fallback.png">$VAR[MovieGenreThumbPos$PARAM[ID]]</texture>
				<visible>SubString(ListItem.Property(Path),movies/genres)</visible>
			</control>
			<control type="image">
				<top>13</top>
				<width>58</width>
				<height>58</height>
				<texture>views/OverlayHD.png</texture>
				<visible>IntegerGreaterThan(ListItem.Property(VideoResolution$PARAM[ID]),719) + !SubString(ListItem.Property(Path),pvr://channels)</visible>
			</control>
			<control type="image">
				<top>13</top>
				<width>308</width>
				<height>308</height>
				<texture border="2">views/season_overlay.png</texture>
