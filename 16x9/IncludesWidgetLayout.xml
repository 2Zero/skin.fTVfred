				<width>318</width>
				<height>334</height>
				<bordersize>0,13,10,13</bordersize>
				<aspectratio align="center">scale</aspectratio>
				<texture background="true" fallback="views/square_fallback.png">$INFO[ListItem.Property(Icon$PARAM[ID])]</texture>
				<visible>!SubString(ListItem.Property(Path),pvr://channels)</visible>
			</control>
			<control type="image">
				<width>318</width>
				<height>334</height>
				<bordersize>0,13,10,13</bordersize>
